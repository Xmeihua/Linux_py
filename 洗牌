我的python作业
#include"stdio.h"
void fan(int a[],int n)
{
	int j,k,i;
	int b[55];
	for(j=1;j<=n;j++)
	{
		for(k=1;k<=27;k++)
		{
			b[2*k-1]=a[k];
			b[2*k]=a[k+27];
		}
	}
		for(i=1;i<55;i++)
		{
			a[k]=b[k];
		}

}
void main()
{
	int a,i,k=2;
	int b[55];
    printf("please input 21 number");
	for(a=1;a<22;a++)
	{
		int m;
		scanf("%d",&m);
		b[a]=m;
	}
	fan(b,k);
	for(i=1;i<=54;i++)
	{
		printf("%d ",b[i]);
	}
}

